{
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "endOfLine": "auto",
  "jsxSingleQuote": false,
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue",
        "htmlWhitespaceSensitivity": "ignore",
        "printWidth": 100
      }
    }
  ]
}
